#include "libft.h"

char	*ft_strnstr(const char *str, const char *to_find, size_t n)
{
	size_t	i;
	size_t	j;

	i = 0;
	if (to_find[0] == '\0')
	{
		return ((char *)str);
	}
	while (str[i] != '\0')
	{
		j = 0;
		while ((to_find[j] != '\0') && (str[i + j] == to_find[j]))
		{
			j++;
			if (i + j > n)
				return ('\0');
		}
		if (to_find[j] == '\0')
			return ((char *)str + i);
		i++;
	}
	return ('\0');
}

