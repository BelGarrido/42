#include "libft.h"

void	*ft_memmove(void *dest, const void *src, size_t count)
{
	char		*d;
	const char	*s = (const char *)src;
	int			i;

	d = (char *)dest;
	i = 0;
	//printf("s+count=%p, s=%p, d=%p\n", s+count, s, d);
	if (!dest && !src)
	return (NULL);
	if (s + count > d)
	{
		//printf("PATRAS\n");
		i = count - 1;
		while (i >= 0)
		{
			d[i] = s[i];
			i--;
		}
	}
	else
	{
		while (i < (int)count)
		{
			//printf("PALANTE\n");
			d[i] = s[i];
			i++;
		}
	}
	return (dest);
}

